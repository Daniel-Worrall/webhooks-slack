{
  "name": "Plex Discord Webhooks",
  "description": "Use Plex webhooks to push media notifications into Discord",
  "repository": "https://github.com/Daniel-Worrall/webhooks-slack",
  "logo": "https://www.plex.tv/wp-content/themes/plex/img/plex-logo@2x.png",
  "keywords": ["node", "redis"],
  "addons": [
    {"plan": "heroku-redis:hobby-dev"}
  ],
  "env": {
    "WEBHOOK_URL": {
      "description": "Discord webhook URL"
    },
    "APP_URL": {
      "description": "URL of this App ({app_name}.herokuapp.com)"
    }
  }
}
